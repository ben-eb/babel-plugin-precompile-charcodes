{
  "name": "babel-plugin-precompile-charcodes",
  "version": "1.1.0",
  "description": "Convert character code getters into plain integers",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "contributorAdd": "all-contributors add",
    "contributorGenerate": "all-contributors generate",
    "prepare": "del-cli dist && BABEL_ENV=publish babel src --out-dir dist --ignore ./src/__tests__",
    "report": "nyc report --reporter=html",
    "pretest": "eslint src",
    "test": "nyc ava src/__tests__"
  },
  "files": [
    "LICENSE-MIT",
    "dist"
  ],
  "keywords": [
    "babel",
    "babel-plugin",
    "charcode",
    "precompile",
    "optimization"
  ],
  "homepage": "https://github.com/ben-eb/babel-plugin-precompile-charcodes",
  "repository": "ben-eb/babel-plugin-precompile-charcodes",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "all-contributors-cli": "^5.4.0",
    "ava": "^1.0.0-beta.8",
    "babel-plugin-add-module-exports": "^1.0.0",
    "coveralls": "^3.0.2",
    "del-cli": "^1.1.0",
    "eslint": "^5.6.1",
    "nyc": "^13.0.1"
  },
  "ava": {
    "require": "@babel/register"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "engines": {
    "node": ">=6"
  }
}
